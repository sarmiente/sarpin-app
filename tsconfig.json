{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
"exclude": [
    "dist"
],
"compilerOptions": {
     "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@/sections/*": ["./src/components/section*"],
      "@/buttons/*": ["./src/components/button*"],
      "@/ui/*": ["./src/components/ui*"],
      "@/*": ["./src/*"],
    },
    "types": [
        "./worker-configuration.d.ts",
        "node"
    ]
}     
} 
